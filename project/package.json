    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/platform-socket.io": "^11.1.0",
    "@nestjs/swagger": "^11.1.5",
    "@nestjs/websockets": "^10.4.17",
    "@prisma/client": "^6.6.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/mocha": "^10.0.10",
    "bcrypt": "^5.1.1",
